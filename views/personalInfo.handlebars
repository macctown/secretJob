<div class="container-wrap">
    <div class="main-content container-fullwidth">
        <div class="row">
            <div class="noo-main col-md-12">
                <div class="jform">
                    {{>resumeHeader}}
                    <div class="jform-body">
                        <div class="container-boxed max">
                            <form class="form-horizontal" enctype="multipart/form-data">
                                <div class="jstep-content">
                                    <div class="jpanel jpanel-resume-form">
                                        <div class="jpanel-title">
                                            <h3>{{title}}</h3>
                                        </div>
                                        <div class="jpanel-body">
                                            <div class="resume-candidate-profile">
                                                <div class="resume-form">
                                                    <div class="resume-form-general row">
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label for="firstName" class="col-sm-5 control-label">First Name</label>
                                                                <div class="col-sm-7">
                                                                    <input type="text" value="" class="form-control" id="firstName" name="firstName" required>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="lastName" class="col-sm-5 control-label">Last Name</label>
                                                                <div class="col-sm-7">
                                                                    <input type="text" value="" class="form-control"  id="lastName" name="lastName" required>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="homeLocation" class="col-sm-5 control-label">Your Location</label>
                                                                <div class="col-sm-7">
                                                                    <input type="text" class="form-control"  id="homeLocation"  name="homeLocation" required>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="timeZone" class="col-sm-5 control-label">Time Zone</label>
                                                                <div class="col-sm-7">
                                                                    <input type="text" class="form-control"  id="timeZone"  name="timeZone" required>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="phone" class="col-sm-5 control-label">Phone</label>
                                                                <div class="col-sm-7">
                                                                    <input type="text" class="form-control" id="phone" name="phone">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <div class="form-group">
                                                                <label for="linked" class="col-sm-3 control-label">Linkedin</label>
                                                                <div class="col-sm-9">
                                                                    <input type="text" class="form-control" id="linkedin" name="phone">
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="github" class="col-sm-3 control-label">Github</label>
                                                                <div class="col-sm-9">
                                                                    <input type="text" class="form-control" id="github" name="phone">
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="website" class="col-sm-3 control-label">Personal Website</label>
                                                                <div class="col-sm-9">
                                                                    <input type="text" class="form-control" id="website" name="phone">
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                            <label for="resume" class="col-sm-3 control-label">Upload your Resume</label>
                                                                <div class="col-sm-9 pull-right upload-to-cv">
                                                                    <p class="help-block">
                                                                    <input id="file_cv" type="file" name="application_attachment" accept=".doc,.docx,.pdf">
                                                                    Allowed file: doc,docx,pdf</p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-actions form-group text-center clearfix">
                                            <a href="/resume/proceedTo/{{calculate step 1 "+"}}" class="btn btn-primary">Save and Next</a>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>